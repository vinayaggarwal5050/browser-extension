{
  "manifest_version": 3,
  "name": "PDF Reader",
  "version": "1.0",
  "description": "Helpful for opening PDF Files.",
  "permissions": ["history", "tabs", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "incognito": "spanning"
}
